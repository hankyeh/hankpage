<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>NUglify</id>
    <version>1.5.13</version>
    <authors>Ron Logan,Alexandre Mutel</authors>
    <owners>Ron Logan,Alexandre Mutel</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/xoofx/NUglify/blob/master/license.txt</licenseUrl>
    <projectUrl>https://github.com/xoofx/NUglify</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xoofx/NUglify/master/images/nuglify.png</iconUrl>
    <description>NUglify provides minify and compression methods for CSS, JavaScript and HTML files for .NET (fork of AjaxMin + new features)</description>
    <releaseNotes>https://github.com/xoofx/NUglify/blob/master/changelog.md</releaseNotes>
    <copyright>Alexandre Mutel</copyright>
    <tags>CSS JavaScript minify crunch uglify Compress HTML htmlminifier htmlcompressor</tags>
